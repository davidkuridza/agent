{
    "package": {
        "name": "agent",
        "repo": "DISTRIBUTION",
        "subject": "BINTRAY_SUBJECT"
    },

    "version": {
        "name": "VERSION",
        "desc": "Release VERSION",
        "released": "DATE",
        "vcs_tag": "VERSION",
        "gpgSign": true
    },

    "files": [{
        "includePattern": "pkg/DISTRIBUTION/(.*(deb|rpm))",
        "uploadPattern": "$1",
        "matrixParams": {
            "deb_distribution": "tactycal",
            "deb_component": "main",
            "deb_architecture": "amd64",
            "override": 1
        }
    }],

    "publish": true
}
